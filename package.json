{
    "name": "SendAfrika",
    "version": "1.82.0",
    "private": true,
    "engines": {
        "node": ">=18"
    },
    "packageManager": "yarn@1.22.19",
    "scripts": {
        "prepare": "is-ci || husky install",
        "postinstall": "patch-package && yarn intl:compile",
        "prebuild": "expo prebuild --clean",
        "android": "expo run:android",
        "ios": "expo run:ios",
        "web": "expo start --web",
        "build-all": "yarn intl:build && yarn use-build-number-with-bump eas build --platform all",
        "build-ios": "yarn use-build-number-with-bump eas build -p ios",
        "build-android": "yarn use-build-number-with-bump eas build -p android",
        "build": "yarn use-build-number-with-bump eas build",
        "build-ios-prod": "NODE_ENV=production eas build -p ios --profile production",
        "build-ios-dev": "NODE_ENV=development eas build -p ios --profile development",
        "build-ios-stag": "NODE_ENV=development eas build -p ios --profile staging",
        "build-android-prod": "NODE_ENV=production eas build -p android --profile production",
        "build-android-dev": "NODE_ENV=development eas build -p android --profile development",
        "build-android-stag": "NODE_ENV=development eas build -p android --profile staging",
        "build-android-prev": "NODE_ENV=development eas build -p android --profile preview",
        "build-ios-prev": "NODE_ENV=development eas build -p ios --profile preview",
        "build-embed": "cd bskyembed && yarn build && yarn build-snippet && cd .. && node ./scripts/post-embed-build.js",
        "start": "expo start --dev-client",
        "start:prod": "expo start --dev-client --no-dev --minify",
        "clean-cache": "rm -rf node_modules cache clean",
        "test": "NODE_ENV=test jest --forceExit --testTimeout=20000 --bail",
        "test-watch": "NODE_ENV=test jest --watchAll",
        "test-ci": "NODE_ENV=test jest --ci --forceExit --reporters=default --reporters=jest-junit",
        "test-coverage": "NODE_ENV=test jest --coverage",
        "lint": "eslint --fix .",
        "typecheck": "tsc --project ./tsconfig.check.json",
        "intl:build": "yarn intl:extract && yarn intl:compile",
        "intl:extract": "lingui extract",
        "intl:compile": "lingui compile",
        "nuke": "rm -rf ./node_modules && rm -rf ./ios && rm -rf ./android",
        "update-extensions": "bash scripts/updateExtensions.sh",
        "export": "npx expo export",
        "make-deploy-bundle": "bash scripts/bundleUpdate.sh",
        "generate-webpack-stats-file": "EXPO_PUBLIC_GENERATE_STATS=1 yarn build-web",
        "open-analyzer": "EXPO_PUBLIC_OPEN_ANALYZER=1 yarn build-web",
        "extract": "lingui extract",
        "compile": "lingui compile",
        "format": "prettier --write \"src/**/*.{tsx,ts,js,jsx}\"",
        "rm-modules": "rm -rf node_modules",
        "start-clear": "expo start --clear"
    },
    "dependencies": {
        "@discord/bottom-sheet": "bluesky-social/react-native-bottom-sheet",
        "@expo/plist": "^0.2.1",
        "@expo/vector-icons": "^14.0.4",
        "@expo/webpack-config": "^19.0.0",
        "@formatjs/intl-locale": "^4.2.5",
        "@formatjs/intl-pluralrules": "^5.3.5",
        "@gorhom/portal": "^1.0.14",
        "@haileyok/bluesky-video": "^0.2.6",
        "@hookform/error-message": "^2.0.1",
        "@hookform/resolvers": "^3.3.4",
        "@lingui/cli": "^4.14.0",
        "@lingui/macro": "^4.14.0",
        "@lingui/react": "^4.14.0",
        "@react-native-async-storage/async-storage": "1.23.1",
        "@react-native-community/netinfo": "^11.4.1",
        "@react-navigation/bottom-tabs": "^6.5.7",
        "@react-navigation/drawer": "^6.6.2",
        "@react-navigation/native": "^6.1.17",
        "@react-navigation/native-stack": "^6.9.12",
        "@reduxjs/toolkit": "^2.3.0",
        "@rn-primitives/avatar": "~1.0.4",
        "@rn-primitives/dropdown-menu": "^1.1.0",
        "@rn-primitives/portal": "~1.0.3",
        "@rn-primitives/progress": "~1.0.3",
        "@rn-primitives/slot": "~1.0.3",
        "@rn-primitives/tabs": "~1.0.3",
        "@rn-primitives/tooltip": "~1.0.3",
        "@rn-primitives/types": "~1.0.3",
        "@sentry/react-native": "^6.8.0",
        "@shopify/flash-list": "1.7.1",
        "@tanstack/query-async-storage-persister": "^5.60.6",
        "@tanstack/react-query": "^5.61.0",
        "@tanstack/react-query-persist-client": "^5.61.0",
        "@types/react-native-crypto-js": "^1.0.3",
        "axios": "^1.7.7",
        "babel-plugin-transform-remove-console": "^6.9.4",
        "bcp-47": "^2.1.0",
        "class-variance-authority": "^0.7.0",
        "clsx": "^2.1.1",
        "date-fns": "^4.1.0",
        "eas-cli": "^9.2.0",
        "email-validator": "^2.0.4",
        "eventemitter3": "^5.0.1",
        "expo": "~52.0.6",
        "expo-blur": "^14.0.1",
        "expo-build-properties": "^0.13.1",
        "expo-clipboard": "^7.0.0",
        "expo-dev-client": "^5.0.4",
        "expo-device": "~7.0.2",
        "expo-haptics": "^14.0.0",
        "expo-linking": "~7.0.2",
        "expo-local-authentication": "~15.0.1",
        "expo-localization": "^16.0.0",
        "expo-navigation-bar": "^4.0.8",
        "expo-splash-screen": "~0.29.9",
        "expo-status-bar": "~2.0.0",
        "expo-system-ui": "~4.0.2",
        "expo-updates": "~0.26.9",
        "expo-web-browser": "~14.0.1",
        "jwt-decode": "^4.0.0",
        "lucide-react-native": "^0.460.0",
        "nativewind": "^4.1.23",
        "new-react-native-countdown-component": "^2.9.0",
        "onesignal-expo-plugin": "^2.0.3",
        "patch-package": "^6.5.1",
        "phone": "^3.1.53",
        "react": "18.3.1",
        "react-dom": "18.3.1",
        "react-hook-form": "^7.53.2",
        "react-native": "0.76.5",
        "react-native-calendars": "^1.1307.0",
        "react-native-crypto-js": "^1.0.0",
        "react-native-gesture-handler": "~2.20.2",
        "react-native-keyboard-controller": "^1.16.4",
        "react-native-onesignal": "^5.2.6",
        "react-native-reanimated": "^3.16.6",
        "react-native-root-siblings": "^5.0.1",
        "react-native-safe-area-context": "4.12.0",
        "react-native-screens": "~4.4.0",
        "react-native-svg": "15.8.0",
        "react-native-toast-notifications": "^3.4.0",
        "react-native-user-inactivity": "^1.2.0",
        "react-native-vector-icons": "^10.0.3",
        "react-native-web": "^0.19.13",
        "react-redux": "^9.1.2",
        "react-responsive": "^10.0.0",
        "round-to": "^6.0.0",
        "tailwind-merge": "^2.5.4",
        "tailwindcss": "^3.4.15"
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/preset-env": "^7.20.0",
        "@babel/runtime": "^7.20.0",
        "@react-native/typescript-config": "^0.76.3",
        "@testing-library/jest-native": "^5.4.1",
        "@testing-library/react-native": "^11.5.2",
        "@types/jest": "^29.4.0",
        "@types/react": "^18.3.12",
        "@types/react-dom": "~18.3.1",
        "babel-jest": "^29.7.0",
        "babel-loader": "^9.1.2",
        "babel-plugin-macros": "^3.1.0",
        "babel-plugin-module-resolver": "^5.0.0",
        "babel-plugin-react-native-web": "^0.18.12",
        "babel-preset-expo": "~12.0.0",
        "eslint": "^8.57.0",
        "eslint-config-expo": "^8.0.1",
        "eslint-config-prettier": "^9.1.0",
        "eslint-plugin-lingui": "^0.8.0",
        "eslint-plugin-prettier": "^5.2.1",
        "husky": "^8.0.3",
        "jest": "^29.7.0",
        "jest-expo": "~52.0.1",
        "jest-junit": "^15.0.0",
        "lint-staged": "^13.2.3",
        "metro-minify-terser": "^0.80.9",
        "metro-react-native-babel-preset": "^0.73.7",
        "prettier": "^3.3.3",
        "react-native-dotenv": "^3.4.11",
        "react-test-renderer": "18.2.0",
        "ts-node": "^10.9.1",
        "typescript": "~5.3.3",
        "webpack-bundle-analyzer": "^4.10.1",
        "webpack-cli": "^5.0.1",
        "webpack-dev-server": "^4.11.1"
    },
    "resolutions": {
        "@types/react": "^18"
    },
    "jest": {
        "preset": "jest-expo/ios",
        "setupFilesAfterEnv": [
            "./jest/jestSetup.js",
            "@testing-library/jest-native/extend-expect"
        ],
        "moduleFileExtensions": [
            "ts",
            "tsx",
            "js",
            "jsx",
            "json",
            "node"
        ],
        "transform": {
            "\\.[jt]sx?$": "babel-jest"
        },
        "transformIgnorePatterns": [
            "node_modules/(?!((jest-)?react-native|@react-native(-community)?)|@discord|expo(nent)?|@expo(nent)?/.*|@expo-google-fonts/.*|react-navigation|nanoid|@react-navigation/.*|@unimodules/.*|unimodules|sentry-expo|native-base|normalize-url|react-native-svg|@sentry/.*|sentry-expo|bcp-47-match)"
        ],
        "modulePathIgnorePatterns": [
            "__tests__/.*/__mocks__",
            "__e2e__/.*"
        ],
        "coveragePathIgnorePatterns": [
            "<rootDir>/node_modules/",
            "<rootDir>/src/platform",
            "<rootDir>/src/third-party",
            "<rootDir>/src/view/com/util",
            "<rootDir>/src/state/lib",
            "<rootDir>/__tests__/test-utils.js"
        ],
        "reporters": [
            "default",
            "jest-junit"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "lint-staged": {
        "*{.js,.jsx,.ts,.tsx}": [
            "eslint --cache --fix",
            "prettier --cache --write --ignore-unknown"
        ]
    }
}
